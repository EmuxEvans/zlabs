<project
    name = "zlabs"
    description = "Labs project for various zmq stuff."
    script = "project.gsl"
    email = "zeromq-dev@lists.zeromq.org" 
    >

    <include filename = "license.xml" />

    <version major = "1" minor = "0" patch = "0" />

    <dependency name="zmq" header="zmq.h" test="zmq_init" />
    <dependency name="czmq" header="czmq.h" test="zctx_test" />

    <!-- 
         <header name = "zlabs" />
    -->

    <class name = "zpubsub" />
    <class name = "zpubsub_option" />
    <class name = "ztimeout" />

    <!-- Models that we build using GSL -->
    <model name = "zpubsub_filter" />
    <class name = "zpubsub_filter" />
</project>
